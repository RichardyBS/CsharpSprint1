          "raw": "{\n  \"id\": \"123e4567-e89b-12d3-a456-426614174000\",\n  \"clienteId\": \"123e4567-e89b-12d3-a456-426614174001\",\n  \"placa\": \"ABC1234\",\n  \"modelo\": \"Honda CB 600F\",\n  \"ano\": 2023,\n  \"cor\": \"Azul\",\n  \"status\": \"Ativo\",\n  \"idVaga\": 15,\n  \"dataEntrada\": \"2025-01-27T10:30:00Z\",\n  \"dataHora\": \"2025-01-27T10:30:00Z\",\n  \"processado\": false\n}"
