{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "StringsConexao": {
    "ConexaoPadrao": "#{ORACLE_CONNECTION_STRING}#",
    "MongoDB": "#{MONGODB_CONNECTION_STRING}#"
  },
  "ConfiguracoesJwt": {
    "ChaveSecreta": "#{JWT_SECRET_KEY}#",
    "Emissor": "#{JWT_ISSUER}#",
    "Audiencia": "#{JWT_AUDIENCE}#",
    "ExpiracaoEmMinutos": "#{JWT_EXPIRATION_MINUTES}#"
  },
  "RabbitMQ": {
    "NomeHost": "#{RABBITMQ_HOST}#",
    "Porta": "#{RABBITMQ_PORT}#",
    "NomeUsuario": "#{RABBITMQ_USERNAME}#",
    "Senha": "#{RABBITMQ_PASSWORD}#",
    "HostVirtual": "#{RABBITMQ_VIRTUAL_HOST}#",
    "NomeExchange": "eventos_estacionamento"
  }
}